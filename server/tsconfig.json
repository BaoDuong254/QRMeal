{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": ["./src/*"],
      "@generated/*": ["./generated/*"]
    }
  },
  "files": ["./src/type.d.ts"],
  "include": ["./src/**/*", "./generated/**/*", "./prisma.config.ts"],
  "exclude": ["node_modules", "dist"]
}
